{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { ref, computed, onMounted } from 'vue';\nimport { BarChart3, ChevronRight, ChevronLeft, Calendar, User, X, FileText } from 'lucide-vue-next';\n\n// 响应式数据\nconst noticesPerPage = 20;\n\n// 模拟公告数据\n\nexport default {\n  __name: 'NoticePage',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const showNoticeModal = ref(false);\n    const selectedNotice = ref(null);\n    const currentPage = ref(1);\n    const allNotices = ref([{\n      id: 1,\n      title: '关于开展2025年度税收大数据分析工作的通知',\n      time: '2025-05-05',\n      author: '税务信息化管理中心',\n      read: false,\n      content: `<p>各省、自治区、直辖市和计划单列市税务局，国家税务总局驻各地特派员办事处，局内各单位：</p>\n              <p>为深入贯彻落实党中央、国务院关于数字化转型的战略部署，充分发挥大数据在税收治理现代化中的重要作用，现就开展2025年度税收大数据分析工作通知如下：</p>\n              <h4>一、工作目标</h4>\n              <p>通过税收大数据分析，提升税收征管精准度，优化纳税服务质量，强化税收风险防控能力，为税收决策提供数据支撑。</p>`,\n      attachments: [{\n        name: '2025年度税收大数据分析工作方案.docx',\n        url: '#',\n        size: '2.3MB'\n      }]\n    }, {\n      id: 2,\n      title: '智税平台V2.3版本更新说明',\n      time: '2025-05-03',\n      author: '税务信息化管理中心',\n      read: false,\n      content: `<p>各位用户：</p><p>智税平台V2.3版本已于2025年5月3日正式上线，本次更新主要内容如下：</p>`,\n      attachments: []\n    }]);\n\n    // 生成更多测试数据\n    for (let i = 3; i <= 50; i++) {\n      allNotices.value.push({\n        id: i,\n        title: `税务系统公告 ${i} - 重要通知事项`,\n        time: `2025-${String(Math.floor(Math.random() * 12) + 1).padStart(2, '0')}-${String(Math.floor(Math.random() * 28) + 1).padStart(2, '0')}`,\n        author: ['税务信息化管理中心', '风险管理司', '税收科学研究所'][Math.floor(Math.random() * 3)],\n        read: Math.random() > 0.7,\n        content: `<p>这是第 ${i} 条公告的详细内容。</p><p>包含重要的税务政策信息和操作指引。</p>`,\n        attachments: Math.random() > 0.5 ? [{\n          name: `相关文件${i}.pdf`,\n          url: '#',\n          size: `${(Math.random() * 5 + 1).toFixed(1)}MB`\n        }] : []\n      });\n    }\n\n    // 计算属性\n    const totalPages = computed(() => {\n      return Math.ceil(allNotices.value.length / noticesPerPage);\n    });\n    const paginatedNotices = computed(() => {\n      const startIndex = (currentPage.value - 1) * noticesPerPage;\n      return allNotices.value.slice(startIndex, startIndex + noticesPerPage);\n    });\n    const displayedPageNumbers = computed(() => {\n      const maxVisiblePages = 5;\n      let startPage = Math.max(1, currentPage.value - Math.floor(maxVisiblePages / 2));\n      let endPage = Math.min(totalPages.value, startPage + maxVisiblePages - 1);\n      if (endPage - startPage + 1 < maxVisiblePages) {\n        startPage = Math.max(1, endPage - maxVisiblePages + 1);\n      }\n      const pages = [];\n      for (let i = startPage; i <= endPage; i++) {\n        pages.push(i);\n      }\n      return pages;\n    });\n\n    // 方法\n    const prevPage = () => {\n      if (currentPage.value > 1) {\n        currentPage.value--;\n        scrollToTop();\n      }\n    };\n    const nextPage = () => {\n      if (currentPage.value < totalPages.value) {\n        currentPage.value++;\n        scrollToTop();\n      }\n    };\n    const goToPage = page => {\n      currentPage.value = page;\n      scrollToTop();\n    };\n    const scrollToTop = () => {\n      window.scrollTo({\n        top: 0,\n        behavior: 'smooth'\n      });\n    };\n    const openNoticeDetail = notice => {\n      selectedNotice.value = notice;\n      showNoticeModal.value = true;\n    };\n    const markNoticeAsRead = () => {\n      if (selectedNotice.value) {\n        selectedNotice.value.read = true;\n        showNoticeModal.value = false;\n      }\n    };\n    const getNoticePreview = content => {\n      const text = content.replace(/<[^>]*>/g, '').replace(/\\s+/g, ' ').trim();\n      return text.length > 100 ? text.substring(0, 100) + '...' : text;\n    };\n\n    // 生命周期\n    onMounted(() => {\n      // 组件挂载后的逻辑\n    });\n    const __returned__ = {\n      showNoticeModal,\n      selectedNotice,\n      currentPage,\n      noticesPerPage,\n      allNotices,\n      totalPages,\n      paginatedNotices,\n      displayedPageNumbers,\n      prevPage,\n      nextPage,\n      goToPage,\n      scrollToTop,\n      openNoticeDetail,\n      markNoticeAsRead,\n      getNoticePreview,\n      ref,\n      computed,\n      onMounted,\n      get BarChart3() {\n        return BarChart3;\n      },\n      get ChevronRight() {\n        return ChevronRight;\n      },\n      get ChevronLeft() {\n        return ChevronLeft;\n      },\n      get Calendar() {\n        return Calendar;\n      },\n      get User() {\n        return User;\n      },\n      get X() {\n        return X;\n      },\n      get FileText() {\n        return FileText;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","BarChart3","ChevronRight","ChevronLeft","Calendar","User","X","FileText","noticesPerPage","showNoticeModal","selectedNotice","currentPage","allNotices","id","title","time","author","read","content","attachments","name","url","size","i","value","push","String","Math","floor","random","padStart","toFixed","totalPages","ceil","length","paginatedNotices","startIndex","slice","displayedPageNumbers","maxVisiblePages","startPage","max","endPage","min","pages","prevPage","scrollToTop","nextPage","goToPage","page","window","scrollTo","top","behavior","openNoticeDetail","notice","markNoticeAsRead","getNoticePreview","text","replace","trim","substring"],"sources":["D:/tax-centre/tax-centre/src/components/NoticePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"tax-center-app bg-gray-50 min-h-screen\">\r\n    <!-- 顶部导航栏 -->\r\n    <header class=\"bg-white shadow-sm\">\r\n      <div class=\"container mx-auto px-4\">\r\n        <div class=\"flex items-center justify-between h-16\">\r\n          <div class=\"flex items-center\">\r\n            <div class=\"flex-shrink-0\">\r\n              <div class=\"flex items-center\">\r\n                <BarChart3 class=\"h-8 w-8 text-blue-600\" />\r\n                <span class=\"ml-2 text-xl font-bold text-gray-900\">台州数智强基平台</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"flex items-center\">\r\n            <router-link\r\n                to=\"/\"\r\n                class=\"px-4 py-2 text-sm font-medium text-blue-600 border border-blue-600 rounded-md hover:bg-blue-50\"\r\n            >\r\n              返回首页\r\n            </router-link>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </header>\r\n\r\n    <!-- 主要内容区 -->\r\n    <main class=\"container mx-auto px-4 py-6\">\r\n      <div class=\"bg-white rounded-lg shadow\">\r\n        <!-- 页面标题 -->\r\n        <div class=\"px-6 py-4 border-b border-gray-200\">\r\n          <h1 class=\"text-2xl font-bold text-gray-900\">公告栏</h1>\r\n          <p class=\"text-sm text-gray-600 mt-1\">查看所有系统公告和通知</p>\r\n        </div>\r\n\r\n        <!-- 公告列表 -->\r\n        <div class=\"p-6\">\r\n          <div class=\"space-y-4\">\r\n            <div\r\n                v-for=\"notice in paginatedNotices\"\r\n                :key=\"notice.id\"\r\n                class=\"border border-gray-200 rounded-lg p-4 hover:border-blue-300 hover:bg-blue-50 transition-colors cursor-pointer\"\r\n                @click=\"openNoticeDetail(notice)\"\r\n            >\r\n              <div class=\"flex items-start justify-between\">\r\n                <div class=\"flex-1\">\r\n                  <div class=\"flex items-center mb-2\">\r\n                    <div :class=\"[\r\n                      'h-2 w-2 rounded-full mr-3',\r\n                      notice.read ? 'bg-gray-300' : 'bg-blue-500'\r\n                    ]\"></div>\r\n                    <h3 class=\"text-lg font-medium text-gray-900\">{{ notice.title }}</h3>\r\n                  </div>\r\n                  <div class=\"flex items-center text-sm text-gray-500 mb-2\">\r\n                    <Calendar class=\"h-4 w-4 mr-1\" />\r\n                    <span>{{ notice.time }}</span>\r\n                    <User class=\"h-4 w-4 ml-4 mr-1\" />\r\n                    <span>{{ notice.author }}</span>\r\n                  </div>\r\n                  <p class=\"text-sm text-gray-600\">{{ getNoticePreview(notice.content) }}</p>\r\n                </div>\r\n                <div class=\"ml-4 flex-shrink-0\">\r\n                  <ChevronRight class=\"h-5 w-5 text-gray-400\" />\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n\r\n          <!-- 分页控件 -->\r\n          <div class=\"flex justify-center mt-8\">\r\n            <nav class=\"flex items-center space-x-2\">\r\n              <button\r\n                  @click=\"prevPage\"\r\n                  :disabled=\"currentPage === 1\"\r\n                  class=\"px-3 py-2 rounded-md text-gray-600 hover:bg-gray-100 disabled:opacity-50 disabled:cursor-not-allowed flex items-center\"\r\n              >\r\n                <ChevronLeft class=\"h-5 w-5 mr-1\" />\r\n                上一页\r\n              </button>\r\n\r\n              <div class=\"flex space-x-1\">\r\n                <button\r\n                    v-for=\"page in displayedPageNumbers\"\r\n                    :key=\"page\"\r\n                    @click=\"goToPage(page)\"\r\n                    :class=\"[\r\n                    'px-3 py-2 rounded-md min-w-[40px]',\r\n                    currentPage === page\r\n                      ? 'bg-blue-600 text-white'\r\n                      : 'text-gray-600 hover:bg-gray-100'\r\n                  ]\"\r\n                >\r\n                  {{ page }}\r\n                </button>\r\n              </div>\r\n\r\n              <button\r\n                  @click=\"nextPage\"\r\n                  :disabled=\"currentPage === totalPages\"\r\n                  class=\"px-3 py-2 rounded-md text-gray-600 hover:bg-gray-100 disabled:opacity-50 disabled:cursor-not-allowed flex items-center\"\r\n              >\r\n                下一页\r\n                <ChevronRight class=\"h-5 w-5 ml-1\" />\r\n              </button>\r\n            </nav>\r\n          </div>\r\n\r\n          <!-- 分页信息 -->\r\n          <div class=\"text-center mt-4 text-sm text-gray-500\">\r\n            共 {{ allNotices.length }} 条公告，第 {{ currentPage }} / {{ totalPages }} 页\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </main>\r\n\r\n    <!-- 公告详情模态框 -->\r\n    <div v-if=\"showNoticeModal\" class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50\">\r\n      <div class=\"bg-white rounded-lg shadow-xl p-6 w-full max-w-4xl max-h-[80vh] overflow-y-auto mx-4\">\r\n        <div class=\"flex justify-between items-center mb-4\">\r\n          <h2 class=\"text-xl font-bold text-gray-900\">公告详情</h2>\r\n          <button @click=\"showNoticeModal = false\" class=\"text-gray-500 hover:text-gray-700\">\r\n            <X class=\"h-5 w-5\" />\r\n          </button>\r\n        </div>\r\n        <div v-if=\"selectedNotice\" class=\"mb-6\">\r\n          <h3 class=\"text-lg font-medium text-gray-900 mb-2\">{{ selectedNotice.title }}</h3>\r\n          <div class=\"flex items-center text-sm text-gray-500 mb-4\">\r\n            <Calendar class=\"h-4 w-4 mr-1\" />\r\n            <span>{{ selectedNotice.time }}</span>\r\n            <User class=\"h-4 w-4 ml-3 mr-1\" />\r\n            <span>{{ selectedNotice.author }}</span>\r\n          </div>\r\n          <div class=\"border-t border-gray-200 pt-4\">\r\n            <div class=\"prose prose-blue max-w-none text-gray-600\" v-html=\"selectedNotice.content\"></div>\r\n          </div>\r\n          <div v-if=\"selectedNotice.attachments && selectedNotice.attachments.length > 0\" class=\"mt-6\">\r\n            <h4 class=\"font-medium text-gray-900 mb-2\">附件</h4>\r\n            <div class=\"space-y-2\">\r\n              <div\r\n                  v-for=\"(attachment, index) in selectedNotice.attachments\"\r\n                  :key=\"index\"\r\n                  class=\"flex items-center p-2 border border-gray-200 rounded-md hover:bg-gray-50\"\r\n              >\r\n                <FileText class=\"h-5 w-5 text-blue-500 mr-2\" />\r\n                <a :href=\"attachment.url\" class=\"text-blue-600 hover:underline\">{{ attachment.name }}</a>\r\n                <span class=\"ml-2 text-xs text-gray-500\">({{ attachment.size }})</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"flex justify-end\">\r\n          <button\r\n              @click=\"markNoticeAsRead\"\r\n              class=\"px-4 py-2 bg-blue-600 text-white rounded-md hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-blue-500\"\r\n          >\r\n            标记为已读\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 底部 -->\r\n    <footer class=\"bg-white border-t border-gray-200 mt-8\">\r\n      <div class=\"container mx-auto px-4 py-6\">\r\n        <div class=\"text-center text-gray-500 text-sm\">\r\n          <p>© 2025 台州市税务局 . 版权所有</p>\r\n          <p class=\"mt-1\">技术支持: 数智强基中心</p>\r\n        </div>\r\n      </div>\r\n    </footer>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, computed, onMounted } from 'vue'\r\nimport {\r\n  BarChart3,\r\n  ChevronRight,\r\n  ChevronLeft,\r\n  Calendar,\r\n  User,\r\n  X,\r\n  FileText\r\n} from 'lucide-vue-next'\r\n\r\n// 响应式数据\r\nconst showNoticeModal = ref(false)\r\nconst selectedNotice = ref(null)\r\nconst currentPage = ref(1)\r\nconst noticesPerPage = 20\r\n\r\n// 模拟公告数据\r\nconst allNotices = ref([\r\n  {\r\n    id: 1,\r\n    title: '关于开展2025年度税收大数据分析工作的通知',\r\n    time: '2025-05-05',\r\n    author: '税务信息化管理中心',\r\n    read: false,\r\n    content: `<p>各省、自治区、直辖市和计划单列市税务局，国家税务总局驻各地特派员办事处，局内各单位：</p>\r\n              <p>为深入贯彻落实党中央、国务院关于数字化转型的战略部署，充分发挥大数据在税收治理现代化中的重要作用，现就开展2025年度税收大数据分析工作通知如下：</p>\r\n              <h4>一、工作目标</h4>\r\n              <p>通过税收大数据分析，提升税收征管精准度，优化纳税服务质量，强化税收风险防控能力，为税收决策提供数据支撑。</p>`,\r\n    attachments: [\r\n      { name: '2025年度税收大数据分析工作方案.docx', url: '#', size: '2.3MB' }\r\n    ]\r\n  },\r\n  {\r\n    id: 2,\r\n    title: '智税平台V2.3版本更新说明',\r\n    time: '2025-05-03',\r\n    author: '税务信息化管理中心',\r\n    read: false,\r\n    content: `<p>各位用户：</p><p>智税平台V2.3版本已于2025年5月3日正式上线，本次更新主要内容如下：</p>`,\r\n    attachments: []\r\n  }\r\n])\r\n\r\n// 生成更多测试数据\r\nfor (let i = 3; i <= 50; i++) {\r\n  allNotices.value.push({\r\n    id: i,\r\n    title: `税务系统公告 ${i} - 重要通知事项`,\r\n    time: `2025-${String(Math.floor(Math.random() * 12) + 1).padStart(2, '0')}-${String(Math.floor(Math.random() * 28) + 1).padStart(2, '0')}`,\r\n    author: ['税务信息化管理中心', '风险管理司', '税收科学研究所'][Math.floor(Math.random() * 3)],\r\n    read: Math.random() > 0.7,\r\n    content: `<p>这是第 ${i} 条公告的详细内容。</p><p>包含重要的税务政策信息和操作指引。</p>`,\r\n    attachments: Math.random() > 0.5 ? [\r\n      { name: `相关文件${i}.pdf`, url: '#', size: `${(Math.random() * 5 + 1).toFixed(1)}MB` }\r\n    ] : []\r\n  })\r\n}\r\n\r\n// 计算属性\r\nconst totalPages = computed(() => {\r\n  return Math.ceil(allNotices.value.length / noticesPerPage)\r\n})\r\n\r\nconst paginatedNotices = computed(() => {\r\n  const startIndex = (currentPage.value - 1) * noticesPerPage\r\n  return allNotices.value.slice(startIndex, startIndex + noticesPerPage)\r\n})\r\n\r\nconst displayedPageNumbers = computed(() => {\r\n  const maxVisiblePages = 5\r\n  let startPage = Math.max(1, currentPage.value - Math.floor(maxVisiblePages / 2))\r\n  let endPage = Math.min(totalPages.value, startPage + maxVisiblePages - 1)\r\n\r\n  if (endPage - startPage + 1 < maxVisiblePages) {\r\n    startPage = Math.max(1, endPage - maxVisiblePages + 1)\r\n  }\r\n\r\n  const pages = []\r\n  for (let i = startPage; i <= endPage; i++) {\r\n    pages.push(i)\r\n  }\r\n\r\n  return pages\r\n})\r\n\r\n// 方法\r\nconst prevPage = () => {\r\n  if (currentPage.value > 1) {\r\n    currentPage.value--\r\n    scrollToTop()\r\n  }\r\n}\r\n\r\nconst nextPage = () => {\r\n  if (currentPage.value < totalPages.value) {\r\n    currentPage.value++\r\n    scrollToTop()\r\n  }\r\n}\r\n\r\nconst goToPage = (page) => {\r\n  currentPage.value = page\r\n  scrollToTop()\r\n}\r\n\r\nconst scrollToTop = () => {\r\n  window.scrollTo({ top: 0, behavior: 'smooth' })\r\n}\r\n\r\nconst openNoticeDetail = (notice) => {\r\n  selectedNotice.value = notice\r\n  showNoticeModal.value = true\r\n}\r\n\r\nconst markNoticeAsRead = () => {\r\n  if (selectedNotice.value) {\r\n    selectedNotice.value.read = true\r\n    showNoticeModal.value = false\r\n  }\r\n}\r\n\r\nconst getNoticePreview = (content) => {\r\n  const text = content.replace(/<[^>]*>/g, '').replace(/\\s+/g, ' ').trim()\r\n  return text.length > 100 ? text.substring(0, 100) + '...' : text\r\n}\r\n\r\n// 生命周期\r\nonMounted(() => {\r\n  // 组件挂载后的逻辑\r\n})\r\n</script>\r\n\r\n<style scoped>\r\n.tax-center-app {\r\n  font-family: 'PingFang SC', 'Helvetica Neue', 'Microsoft YaHei', sans-serif;\r\n}\r\n\r\n.prose {\r\n  line-height: 1.6;\r\n}\r\n\r\n.prose h4 {\r\n  font-weight: 600;\r\n  margin-top: 1.5em;\r\n  margin-bottom: 0.5em;\r\n  font-size: 1.1em;\r\n}\r\n\r\n.prose p {\r\n  margin-bottom: 1em;\r\n}\r\n</style>"],"mappings":";AA8KA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,QAAQ,KAAK;AAC9C,SACEC,SAAS,EACTC,YAAY,EACZC,WAAW,EACXC,QAAQ,EACRC,IAAI,EACJC,CAAC,EACDC,QAAQ,QACH,iBAAiB;;AAExB;AAIA,MAAMC,cAAc,GAAG,EAAE;;AAEzB;;;;;;;;IALA,MAAMC,eAAe,GAAGX,GAAG,CAAC,KAAK,CAAC;IAClC,MAAMY,cAAc,GAAGZ,GAAG,CAAC,IAAI,CAAC;IAChC,MAAMa,WAAW,GAAGb,GAAG,CAAC,CAAC,CAAC;IAI1B,MAAMc,UAAU,GAAGd,GAAG,CAAC,CACrB;MACEe,EAAE,EAAE,CAAC;MACLC,KAAK,EAAE,wBAAwB;MAC/BC,IAAI,EAAE,YAAY;MAClBC,MAAM,EAAE,WAAW;MACnBC,IAAI,EAAE,KAAK;MACXC,OAAO,EAAE;AACb;AACA;AACA,0EAA0E;MACtEC,WAAW,EAAE,CACX;QAAEC,IAAI,EAAE,wBAAwB;QAAEC,GAAG,EAAE,GAAG;QAAEC,IAAI,EAAE;MAAQ,CAAC;IAE/D,CAAC,EACD;MACET,EAAE,EAAE,CAAC;MACLC,KAAK,EAAE,gBAAgB;MACvBC,IAAI,EAAE,YAAY;MAClBC,MAAM,EAAE,WAAW;MACnBC,IA