{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport { ref, computed, onMounted, watch } from 'vue';\nimport { BarChart3, Search, Bell, ChevronDown, ChevronRight, FileText, PieChart, Users, Database, BarChart, Calculator, FileCheck, Building, BookOpen, CreditCard, Landmark, Receipt, TrendingUp, AlertCircle, HelpCircle, X, Eye, Brain, UserCog, Monitor, Flag, Globe, Presentation, MessageSquare, Star, LifeBuoy, Briefcase, Scale } from 'lucide-vue-next';\nimport axios from 'axios';\n\n// 用户登录状态\n\nexport default {\n  __name: 'TaxCenter',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const currentUser = ref(null);\n    const showLoginModal = ref(false);\n    const loginForm = ref({\n      username: '',\n      password: ''\n    });\n    const loginError = ref('');\n    const isLoggingIn = ref(false);\n    const showUserMenu = ref(false);\n\n    // 查看全部功能模态框\n    const showAllAppsModal = ref(false);\n\n    // 收藏夹\n    const favorites = ref([]);\n\n    // 检查是否已登录和加载收藏夹\n    onMounted(() => {\n      // 检查登录状态\n      const savedUser = localStorage.getItem('currentUser');\n      if (savedUser) {\n        try {\n          currentUser.value = JSON.parse(savedUser);\n        } catch (e) {\n          localStorage.removeItem('currentUser');\n        }\n      }\n\n      // 加载收藏夹\n      const savedFavorites = localStorage.getItem('favorites');\n      if (savedFavorites) {\n        try {\n          favorites.value = JSON.parse(savedFavorites);\n        } catch (e) {\n          localStorage.removeItem('favorites');\n        }\n      }\n    });\n\n    // 收藏功能\n    const toggleFavorite = app => {\n      const index = favorites.value.findIndex(fav => fav.name === app.name);\n      if (index === -1) {\n        // 添加到收藏\n        favorites.value.push(app);\n      } else {\n        // 从收藏中移除\n        favorites.value.splice(index, 1);\n      }\n      // 保存到本地存储\n      localStorage.setItem('favorites', JSON.stringify(favorites.value));\n    };\n\n    // 检查应用是否已收藏\n    const isFavorite = app => {\n      return favorites.value.some(fav => fav.name === app.name);\n    };\n\n    // 登录处理\n    const handleLogin = async () => {\n      try {\n        isLoggingIn.value = true;\n        loginError.value = '';\n\n        // 调用后端API进行登录验证\n        const response = await axios.post('/api/login', {\n          username: loginForm.value.username,\n          password: loginForm.value.password\n        });\n\n        // 登录成功\n        currentUser.value = {\n          id: response.data.id,\n          username: response.data.username,\n          role: response.data.role\n        };\n\n        // 保存用户信息到本地存储\n        localStorage.setItem('currentUser', JSON.stringify(currentUser.value));\n\n        // 关闭登录模态框\n        showLoginModal.value = false;\n\n        // 重置表单\n        loginForm.value = {\n          username: '',\n          password: ''\n        };\n      } catch (error) {\n        console.error('登录失败:', error);\n        loginError.value = error.response?.data?.message || '登录失败，请检查用户名和密码';\n      } finally {\n        isLoggingIn.value = false;\n      }\n    };\n\n    // 退出登录\n    const handleLogout = () => {\n      currentUser.value = null;\n      localStorage.removeItem('currentUser');\n      showUserMenu.value = false;\n    };\n\n    // 用户菜单状态\n    const toggleUserMenu = () => {\n      showUserMenu.value = !showUserMenu.value;\n    };\n\n    // 搜索\n    const searchQuery = ref('');\n\n    // 通知\n    const notifications = [{\n      content: '关于开展2025年度税收大数据分析工作的通知',\n      time: '2025-05-05'\n    }, {\n      content: '智税平台V2.3版本更新说明',\n      time: '2025-05-03'\n    }, {\n      content: '关于加强税收风险管理的工作指引',\n      time: '2025-04-28'\n    }, {\n      content: '第三季度税收数据分析报告已发布',\n      time: '2025-04-25'\n    }];\n\n    // 全部应用\n    const allApplications = [{\n      name: '指标全生命周期管理',\n      icon: LifeBuoy,\n      color: 'blue',\n      description: '指标管理与监控平台',\n      url: '#'\n    }, {\n      name: '智税展示',\n      icon: Presentation,\n      color: 'green',\n      description: '税务数据可视化展示',\n      url: '#'\n    }, {\n      name: '税费服务诉求',\n      icon: MessageSquare,\n      color: 'purple',\n      description: '税费服务与诉求处理',\n      url: '#'\n    }, {\n      name: '税费融合',\n      icon: CreditCard,\n      color: 'orange',\n      description: '税费业务一体化管理',\n      url: '#'\n    }, {\n      name: '慧眼',\n      icon: Eye,\n      color: 'red',\n      description: '税务风险智能监控',\n      url: '#'\n    }, {\n      name: '数智人事',\n      icon: UserCog,\n      color: 'indigo',\n      description: '人事管理数字化平台',\n      url: '#'\n    }, {\n      name: '定期定额进项监控',\n      icon: Monitor,\n      color: 'yellow',\n      description: '定期定额业务监控',\n      url: '#'\n    }, {\n      name: '智慧党建',\n      icon: Flag,\n      color: 'pink',\n      description: '党建工作数字化平台',\n      url: '#'\n    }, {\n      name: '跨区域管理',\n      icon: Globe,\n      color: 'teal',\n      description: '跨区域税务协同管理',\n      url: '#'\n    }, {\n      name: '案管系统',\n      icon: Briefcase,\n      color: 'cyan',\n      description: '税务案件管理系统',\n      url: '#'\n    }];\n\n    // 应用分类\n    const categories = [{\n      id: 'all',\n      name: '全部'\n    }, {\n      id: 'collection',\n      name: '征收管理'\n    }, {\n      id: 'inspection',\n      name: '稽查管理'\n    }, {\n      id: 'service',\n      name: '纳税服务'\n    }, {\n      id: 'analysis',\n      name: '数据分析'\n    }, {\n      id: 'risk',\n      name: '风险管理'\n    }, {\n      id: 'policy',\n      name: '政策法规'\n    }];\n    const activeCategory = ref('all');\n\n    // 所有应用\n    const allApps = [\n    // 征收管理\n    {\n      name: '申报管理系统',\n      icon: FileText,\n      color: 'blue',\n      category: 'collection',\n      description: '纳税申报与管理',\n      url: '#'\n    }, {\n      name: '发票管理平台',\n      icon: Receipt,\n      color: 'green',\n      category: 'collection',\n      description: '发票开具与管理',\n      url: '#'\n    }, {\n      name: '税款征收系统',\n      icon: CreditCard,\n      color: 'indigo',\n      category: 'collection',\n      description: '税款征收与入库',\n      url: '#'\n    }, {\n      name: '减免税管理',\n      icon: Calculator,\n      color: 'red',\n      category: 'collection',\n      description: '税收减免申请与审批',\n      url: '#'\n    }, {\n      name: '税费融合管理',\n      icon: Scale,\n      color: 'purple',\n      category: 'collection',\n      description: '税费一体化管理',\n      url: '#'\n    }, {\n      name: '定期定额管理',\n      icon: Monitor,\n      color: 'yellow',\n      category: 'collection',\n      description: '定期定额业务管理',\n      url: '#'\n    },\n    // 稽查管理\n    {\n      name: '稽查案件管理',\n      icon: FileCheck,\n      color: 'orange',\n      category: 'inspection',\n      description: '税务稽查案件管理',\n      url: '#'\n    }, {\n      name: '税收风险监控',\n      icon: AlertCircle,\n      color: 'red',\n      category: 'inspection',\n      description: '税收风险实时监控',\n      url: '#'\n    }, {\n      name: '稽查分析工具',\n      icon: PieChart,\n      color: 'purple',\n      category: 'inspection',\n      description: '稽查数据分析工具',\n      url: '#'\n    }, {\n      name: '案管系统',\n      icon: Briefcase,\n      color: 'cyan',\n      category: 'inspection',\n      description: '税务案件管理系统',\n      url: '#'\n    }, {\n      name: '跨区域协查',\n      icon: Globe,\n      color: 'teal',\n      category: 'inspection',\n      description: '跨区域稽查协作',\n      url: '#'\n    },\n    // 纳税服务\n    {\n      name: '纳税人服务平台',\n      icon: Users,\n      color: 'blue',\n      category: 'service',\n      description: '纳税人在线服务',\n      url: '#'\n    }, {\n      name: '税收政策咨询',\n      icon: HelpCircle,\n      color: 'green',\n      category: 'service',\n      description: '税收政策在线咨询',\n      url: '#'\n    }, {\n      name: '办税指南',\n      icon: BookOpen,\n      color: 'indigo',\n      category: 'service',\n      description: '办税流程与指南',\n      url: '#'\n    }, {\n      name: '税费服务诉求',\n      icon: MessageSquare,\n      color: 'purple',\n      category: 'service',\n      description: '税费服务与诉求处理',\n      url: '#'\n    }, {\n      name: '智能客服',\n      icon: MessageSquare,\n      color: 'pink',\n      category: 'service',\n      description: '智能问答服务',\n      url: '#'\n    },\n    // 数据分析\n    {\n      name: '税收数据分析',\n      icon: BarChart,\n      color: 'purple',\n      category: 'analysis',\n      description: '税收数据多维分析',\n      url: '#'\n    }, {\n      name: '税收预测模型',\n      icon: TrendingUp,\n      color: 'green',\n      category: 'analysis',\n      description: '税收趋势预测',\n      url: '#'\n    }, {\n      name: '区域税收分析',\n      icon: PieChart,\n      color: 'blue',\n      category: 'analysis',\n      description: '区域税收对比分析',\n      url: '#'\n    }, {\n      name: '行业税负分析',\n      icon: BarChart3,\n      color: 'orange',\n      category: 'analysis',\n      description: '行业税负水平分析',\n      url: '#'\n    }, {\n      name: '智税展示',\n      icon: Presentation,\n      color: 'green',\n      category: 'analysis',\n      description: '税务数据可视化展示',\n      url: '#'\n    }, {\n      name: '指标全生命周期管理',\n      icon: LifeBuoy,\n      color: 'blue',\n      category: 'analysis',\n      description: '指标管理与监控平台',\n      url: '#'\n    },\n    // 风险管理\n    {\n      name: '风险识别系统',\n      icon: AlertCircle,\n      color: 'red',\n      category: 'risk',\n      description: '税收风险智能识别',\n      url: '#'\n    }, {\n      name: '纳税人信用评级',\n      icon: Users,\n      color: 'blue',\n      category: 'risk',\n      description: '纳税人信用等级评定',\n      url: '#'\n    }, {\n      name: '风险应对策略',\n      icon: FileCheck,\n      color: 'green',\n      category: 'risk',\n      description: '税收风险应对策略',\n      url: '#'\n    }, {\n      name: '慧眼',\n      icon: Eye,\n      color: 'red',\n      category: 'risk',\n      description: '税务风险智能监控',\n      url: '#'\n    }, {\n      name: '大数据风控',\n      icon: Database,\n      color: 'indigo',\n      category: 'risk',\n      description: '大数据风险控制',\n      url: '#'\n    },\n    // 政策法规\n    {\n      name: '税收法规库',\n      icon: BookOpen,\n      color: 'indigo',\n      category: 'policy',\n      description: '税收法律法规查询',\n      url: '#'\n    }, {\n      name: '政策解读平台',\n      icon: FileText,\n      color: 'purple',\n      category: 'policy',\n      description: '税收政策专业解读',\n      url: '#'\n    }, {\n      name: '税收案例库',\n      icon: Landmark,\n      color: 'blue',\n      category: 'policy',\n      description: '典型税收案例分析',\n      url: '#'\n    }, {\n      name: '政策执行指南',\n      icon: FileCheck,\n      color: 'green',\n      category: 'policy',\n      description: '政策执行操作指南',\n      url: '#'\n    }, {\n      name: '税收条约库',\n      icon: Globe,\n      color: 'teal',\n      category: 'policy',\n      description: '国际税收条约查询',\n      url: '#'\n    }];\n\n    // 根据分类筛选应用\n    const filteredApps = computed(() => {\n      if (activeCategory.value === 'all') {\n        return allApps;\n      } else {\n        return allApps.filter(app => app.category === activeCategory.value);\n      }\n    });\n\n    // 限制显示3排应用\n    const limitedFilteredApps = computed(() => {\n      const appsPerRow = 3; // 每行显示3个应用\n      const maxRows = 3; // 最多显示3行\n      return filteredApps.value.slice(0, appsPerRow * maxRows);\n    });\n    const __returned__ = {\n      currentUser,\n      showLoginModal,\n      loginForm,\n      loginError,\n      isLoggingIn,\n      showUserMenu,\n      showAllAppsModal,\n      favorites,\n      toggleFavorite,\n      isFavorite,\n      handleLogin,\n      handleLogout,\n      toggleUserMenu,\n      searchQuery,\n      notifications,\n      allApplications,\n      categories,\n      activeCategory,\n      allApps,\n      filteredApps,\n      limitedFilteredApps,\n      ref,\n      computed,\n      onMounted,\n      watch,\n      get BarChart3() {\n        return BarChart3;\n      },\n      get Search() {\n        return Search;\n      },\n      get Bell() {\n        return Bell;\n      },\n      get ChevronDown() {\n        return ChevronDown;\n      },\n      get ChevronRight() {\n        return ChevronRight;\n      },\n      get FileText() {\n        return FileText;\n      },\n      get PieChart() {\n        return PieChart;\n      },\n      get Users() {\n        return Users;\n      },\n      get Database() {\n        return Database;\n      },\n      get BarChart() {\n        return BarChart;\n      },\n      get Calculator() {\n        return Calculator;\n      },\n      get FileCheck() {\n        return FileCheck;\n      },\n      get Building() {\n        return Building;\n      },\n      get BookOpen() {\n        return BookOpen;\n      },\n      get CreditCard() {\n        return CreditCard;\n      },\n      get Landmark() {\n        return Landmark;\n      },\n      get Receipt() {\n        return Receipt;\n      },\n      get TrendingUp() {\n        return TrendingUp;\n      },\n      get AlertCircle() {\n        return AlertCircle;\n      },\n      get HelpCircle() {\n        return HelpCircle;\n      },\n      get X() {\n        return X;\n      },\n      get Eye() {\n        return Eye;\n      },\n      get Brain() {\n        return Brain;\n      },\n      get UserCog() {\n        return UserCog;\n      },\n      get Monitor() {\n        return Monitor;\n      },\n      get Flag() {\n        return Flag;\n      },\n      get Globe() {\n        return Globe;\n      },\n      get Presentation() {\n        return Presentation;\n      },\n      get MessageSquare() {\n        return MessageSquare;\n      },\n      get Star() {\n        return Star;\n      },\n      get LifeBuoy() {\n        return LifeBuoy;\n      },\n      get Briefcase() {\n        return Briefcase;\n      },\n      get Scale() {\n        return Scale;\n      },\n      get axios() {\n        return axios;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","watch","BarChart3","Search","Bell","ChevronDown","ChevronRight","FileText","PieChart","Users","Database","BarChart","Calculator","FileCheck","Building","BookOpen","CreditCard","Landmark","Receipt","TrendingUp","AlertCircle","HelpCircle","X","Eye","Brain","UserCog","Monitor","Flag","Globe","Presentation","MessageSquare","Star","LifeBuoy","Briefcase","Scale","axios","currentUser","showLoginModal","loginForm","username","password","loginError","isLoggingIn","showUserMenu","showAllAppsModal","favorites","savedUser","localStorage","getItem","value","JSON","parse","e","removeItem","savedFavorites","toggleFavorite","app","index","findIndex","fav","name","push","splice","setItem","stringify","isFavorite","some","handleLogin","response","post","id","data","role","error","console","message","handleLogout","toggleUserMenu","searchQuery","notifications","content","time","allApplications","icon","color","description","url","categories","activeCategory","allApps","category","filteredApps","filter","limitedFilteredApps","appsPerRow","maxRows","slice"],"sources":["C:/Users/fatbear/Desktop/tax-center/src/components/TaxCenter.vue"],"sourcesContent":["import { login, logout } from '@/api/login.js'\nimport { addFavorite, removeFavorite, getFavorites } from '@/api/favorite.js'\n\n<template>\n  <div class=\"tax-center-app bg-gray-50 min-h-screen\">\n    <!-- 登录模态框 -->\n    <div v-if=\"showLoginModal\" class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50\">\n      <div class=\"bg-white rounded-lg shadow-xl p-6 w-full max-w-md\">\n        <div class=\"flex justify-between items-center mb-4\">\n          <h2 class=\"text-xl font-bold text-gray-900\">用户登录</h2>\n          <button @click=\"showLoginModal = false\" class=\"text-gray-500 hover:text-gray-700\">\n            <X class=\"h-5 w-5\" />\n          </button>\n        </div>\n        <div v-if=\"loginError\" class=\"mb-4 p-3 bg-red-100 text-red-700 rounded-md\">\n          {{ loginError }}\n        </div>\n        <form @submit.prevent=\"handleLogin\">\n          <div class=\"mb-4\">\n            <label for=\"username\" class=\"block text-sm font-medium text-gray-700 mb-1\">用户名</label>\n            <input \n              v-model=\"loginForm.username\" \n              type=\"text\" \n              id=\"username\" \n              class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n              placeholder=\"请输入用户名\"\n              required\n            />\n          </div>\n          <div class=\"mb-6\">\n            <label for=\"password\" class=\"block text-sm font-medium text-gray-700 mb-1\">密码</label>\n            <input \n              v-model=\"loginForm.password\" \n              type=\"password\" \n              id=\"password\" \n              class=\"w-full px-3 py-2 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-500\"\n              placeholder=\"请输入密码\"\n              required\n            />\n          </div>\n          <button \n            type=\"submit\" \n            class=\"w-full bg-blue-600 text-white py-2 px-4 rounded-md hover:bg-blue-700 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:ring-offset-2\"\n            :disabled=\"isLoggingIn\"\n          >\n            {{ isLoggingIn ? '登录中...' : '登录' }}\n          </button>\n        </form>\n      </div>\n    </div>\n\n    <!-- 查看全部功能模态框 -->\n    <div v-if=\"showAllAppsModal\" class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center z-50\">\n      <div class=\"bg-white rounded-lg shadow-xl p-6 w-full max-w-5xl max-h-[80vh] overflow-y-auto\">\n        <div class=\"flex justify-between items-center mb-4\">\n          <h2 class=\"text-xl font-bold text-gray-900\">全部功能</h2>\n          <button @click=\"showAllAppsModal = false\" class=\"text-gray-500 hover:text-gray-700\">\n            <X class=\"h-5 w-5\" />\n          </button>\n        </div>\n        \n        <div class=\"grid grid-cols-2 sm:grid-cols-3 md:grid-cols-4 lg:grid-cols-5 gap-4\">\n          <div\n            v-for=\"(app, index) in allApps\"\n            :key=\"index\"\n            class=\"border border-gray-200 rounded-lg p-4 hover:border-blue-300 hover:bg-blue-50 transition-colors cursor-pointer relative\"\n          >\n            <a :href=\"app.url || '#'\" :target=\"app.url ? '_blank' : ''\" class=\"block no-underline\">\n              <div class=\"flex items-start\">\n                <div :class=\"`bg-${app.color}-100 p-2 rounded-lg`\">\n                  <component :is=\"app.icon\" class=\"h-5 w-5\" :class=\"`text-${app.color}-600`\" />\n                </div>\n                <div class=\"ml-3\">\n                  <h3 class=\"font-medium text-gray-900\">{{ app.name }}</h3>\n                  <p class=\"text-xs text-gray-500 mt-1\">{{ app.description }}</p>\n                </div>\n              </div>\n            </a>\n            <button \n              @click.stop=\"toggleFavorite(app)\"\n              class=\"absolute bottom-2 right-2 text-gray-400 hover:text-yellow-500 focus:outline-none\"\n              :class=\"{ 'text-yellow-500': isFavorite(app) }\"\n            >\n              <Star class=\"h-4 w-4\" :fill=\"isFavorite(app) ? 'currentColor' : 'none'\" />\n            </button>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 顶部导航栏 -->\n    <header class=\"bg-white shadow-sm\">\n      <div class=\"container mx-auto px-4\">\n        <div class=\"flex items-center justify-between h-16\">\n          <div class=\"flex items-center\">\n            <div class=\"flex-shrink-0\">\n              <div class=\"flex items-center\">\n                <BarChart3 class=\"h-8 w-8 text-blue-600\" />\n                <span class=\"ml-2 text-xl font-bold text-gray-900\">强基中心</span>\n              </div>\n            </div>\n            <!-- 导航链接已移除 -->\n          </div>\n          <div class=\"flex items-center\">\n            <div class=\"relative\">\n              <input\n                v-model=\"searchQuery\"\n                type=\"text\"\n                placeholder=\"搜索应用...\"\n                class=\"bg-gray-100 rounded-full py-2 pl-10 pr-4 w-64 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:bg-white\"\n              />\n              <div class=\"absolute left-3 top-2.5\">\n                <Search class=\"h-5 w-5 text-gray-400\" />\n              </div>\n            </div>\n            <div class=\"ml-4 flex items-center\">\n              <Bell class=\"h-6 w-6 text-gray-500 hover:text-blue-600 cursor-pointer\" />\n              <!-- 用户头像和下拉菜单 -->\n              <div class=\"ml-4 relative\">\n                <div v-if=\"currentUser\" class=\"flex items-center cursor-pointer\" @click=\"toggleUserMenu\">\n                  <img\n                    class=\"h-8 w-8 rounded-full\"\n                    src=\"/images/1.jpg\"\n                    alt=\"用户头像\"\n                  />\n                  <span class=\"ml-2 text-sm font-medium text-gray-700 hidden md:block\">{{ currentUser.username }}</span>\n                  <ChevronDown class=\"ml-1 h-4 w-4 text-gray-500\" />\n                </div>\n                <button \n                  v-else \n                  @click=\"showLoginModal = true\"\n                  class=\"ml-2 px-4 py-1 text-sm font-medium text-blue-600 border border-blue-600 rounded-md hover:bg-blue-50\"\n                >\n                  登录\n                </button>\n                <div\n                  v-if=\"showUserMenu && currentUser\"\n                  class=\"origin-top-right absolute right-0 mt-2 w-48 rounded-md shadow-lg bg-white ring-1 ring-black ring-opacity-5 z-10\"\n                >\n                  <div class=\"py-1\">\n                    <a @click=\"handleLogout\" class=\"block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100 cursor-pointer\">退出登录</a>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </header>\n\n    <!-- 主要内容区 -->\n    <main class=\"container mx-auto px-4 py-6\">\n      <div class=\"flex flex-col md:flex-row\">\n        <!-- 左侧边栏 -->\n        <div class=\"w-full md:w-64 mb-6 md:mb-0 md:mr-6\">\n          <!-- 公告栏移到上面 -->\n          <div class=\"bg-white rounded-lg shadow p-4 mb-6\">\n            <h3 class=\"text-lg font-medium text-gray-900 mb-3\">公告栏</h3>\n            <div class=\"space-y-3\">\n              <div v-for=\"(notice, index) in notifications\" :key=\"index\" class=\"border-b border-gray-100 pb-3 last:border-0 last:pb-0\">\n                <div class=\"text-sm text-gray-600\">{{ notice.content }}</div>\n                <div class=\"text-xs text-gray-400 mt-1\">{{ notice.time }}</div>\n              </div>\n            </div>\n          </div>\n          \n          <!-- 收藏夹 -->\n          <div class=\"bg-white rounded-lg shadow p-4\">\n            <h3 class=\"text-lg font-medium text-gray-900 mb-3\">收藏夹</h3>\n            <ul class=\"space-y-2\">\n              <li v-for=\"(app, index) in favorites\" :key=\"index\" class=\"relative\">\n                <a :href=\"app.url || '#'\" :target=\"app.url ? '_blank' : ''\" class=\"flex items-center text-gray-600 hover:text-blue-600 py-1.5 pr-8\">\n                  <component :is=\"app.icon\" class=\"h-5 w-5 mr-2\" :class=\"`text-${app.color}-600`\" />\n                  <span>{{ app.name }}</span>\n                </a>\n                <button \n                  @click.stop=\"toggleFavorite(app)\"\n                  class=\"absolute right-0 top-1/2 transform -translate-y-1/2 text-yellow-500 hover:text-yellow-600 focus:outline-none\"\n                >\n                  <Star class=\"h-4 w-4\" fill=\"currentColor\" />\n                </button>\n              </li>\n              <li v-if=\"favorites.length === 0\" class=\"text-sm text-gray-500 italic py-2\">\n                暂无收藏，点击应用右下角星标添加\n              </li>\n            </ul>\n          </div>\n        </div>\n\n        <!-- 主内容区 -->\n        <div class=\"flex-1\">\n          <!-- 轮播图 -->\n          <div class=\"bg-white rounded-lg shadow mb-6 overflow-hidden\">\n            <div class=\"relative h-64\">\n              <div class=\"absolute inset-0 flex\">\n                <div class=\"w-full h-full bg-gradient-to-r from-blue-600 to-blue-800 flex items-center\">\n                  <div class=\"px-8 md:px-12 w-full md:w-1/2\">\n                    <h2 class=\"text-2xl md:text-3xl font-bold text-white mb-4\">智慧税务 数字赋能</h2>\n                    <p class=\"text-white text-opacity-90 mb-6\">\n                      整合税务系统资源，提供一站式税务服务平台，助力税收治理现代化\n                    </p>\n                    <button class=\"bg-white text-blue-600 px-4 py-2 rounded-md font-medium hover:bg-gray-100\">\n                      了解更多\n                    </button>\n                  </div>\n                </div>\n                <div class=\"hidden md:block w-1/2 h-full bg-center bg-cover\" style=\"background-image: url('/images/2.avif')\"></div>\n              </div>\n            </div>\n          </div>\n\n          <!-- 全部应用 -->\n          <div class=\"mb-6\">\n            <div class=\"flex items-center justify-between mb-4\">\n              <h2 class=\"text-xl font-bold text-gray-900\">全部应用</h2>\n            </div>\n            <div class=\"grid grid-cols-2 sm:grid-cols-3 md:grid-cols-4 lg:grid-cols-5 gap-4\">\n              <div\n                v-for=\"(app, index) in allApplications\"\n                :key=\"index\"\n                class=\"bg-white rounded-lg shadow p-4 hover:shadow-md transition-shadow cursor-pointer relative\"\n              >\n                <a :href=\"app.url || '#'\" :target=\"app.url ? '_blank' : ''\" class=\"block no-underline\">\n                  <div class=\"flex flex-col items-center\">\n                    <div :class=\"`bg-${app.color}-100 p-3 rounded-lg mb-3`\">\n                      <component :is=\"app.icon\" class=\"h-6 w-6\" :class=\"`text-${app.color}-600`\" />\n                    </div>\n                    <div class=\"text-center\">\n                      <h3 class=\"font-medium text-gray-900\">{{ app.name }}</h3>\n                      <p class=\"text-xs text-gray-500 mt-1\">{{ app.description }}</p>\n                    </div>\n                  </div>\n                </a>\n                <button \n                  @click.stop=\"toggleFavorite(app)\"\n                  class=\"absolute bottom-2 right-2 text-gray-400 hover:text-yellow-500 focus:outline-none\"\n                  :class=\"{ 'text-yellow-500': isFavorite(app) }\"\n                >\n                  <Star class=\"h-4 w-4\" :fill=\"isFavorite(app) ? 'currentColor' : 'none'\" />\n                </button>\n              </div>\n            </div>\n          </div>\n\n          <!-- 应用分类 -->\n          <div>\n            <div class=\"flex items-center justify-between mb-4\">\n              <h2 class=\"text-xl font-bold text-gray-900\">应用分类</h2>\n              <button \n                @click=\"showAllAppsModal = true\" \n                class=\"text-sm text-blue-600 hover:text-blue-700 flex items-center\"\n              >\n                查看全部功能 <ChevronRight class=\"h-4 w-4 ml-1\" />\n              </button>\n            </div>\n            \n            <div class=\"bg-white rounded-lg shadow overflow-hidden\">\n              <div class=\"border-b border-gray-200\">\n                <div class=\"flex overflow-x-auto\">\n                  <button\n                    v-for=\"(category, index) in categories\"\n                    :key=\"index\"\n                    @click=\"activeCategory = category.id\"\n                    :class=\"[\n                      'px-4 py-3 text-sm font-medium whitespace-nowrap',\n                      activeCategory === category.id\n                        ? 'text-blue-600 border-b-2 border-blue-600'\n                        : 'text-gray-500 hover:text-gray-700'\n                    ]\"\n                  >\n                    {{ category.name }}\n                  </button>\n                </div>\n              </div>\n              \n              <div class=\"p-4\">\n                <div class=\"grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 gap-4\">\n                  <div\n                    v-for=\"(app, index) in limitedFilteredApps\"\n                    :key=\"index\"\n                    class=\"border border-gray-200 rounded-lg p-4 hover:border-blue-300 hover:bg-blue-50 transition-colors cursor-pointer relative\"\n                  >\n                    <a :href=\"app.url || '#'\" :target=\"app.url ? '_blank' : ''\" class=\"block no-underline\">\n                      <div class=\"flex items-start\">\n                        <div :class=\"`bg-${app.color}-100 p-2 rounded-lg`\">\n                          <component :is=\"app.icon\" class=\"h-5 w-5\" :class=\"`text-${app.color}-600`\" />\n                        </div>\n                        <div class=\"ml-3\">\n                          <h3 class=\"font-medium text-gray-900\">{{ app.name }}</h3>\n                          <p class=\"text-xs text-gray-500 mt-1\">{{ app.description }}</p>\n                        </div>\n                      </div>\n                    </a>\n                    <button \n                      @click.stop=\"toggleFavorite(app)\"\n                      class=\"absolute bottom-2 right-2 text-gray-400 hover:text-yellow-500 focus:outline-none\"\n                      :class=\"{ 'text-yellow-500': isFavorite(app) }\"\n                    >\n                      <Star class=\"h-4 w-4\" :fill=\"isFavorite(app) ? 'currentColor' : 'none'\" />\n                    </button>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </main>\n\n    <!-- 底部 -->\n    <footer class=\"bg-white border-t border-gray-200 mt-8\">\n      <div class=\"container mx-auto px-4 py-6\">\n        <div class=\"text-center text-gray-500 text-sm\">\n          <p>© 2025 国家税务总局 智税中心. 版权所有</p>\n          <p class=\"mt-1\">技术支持: 税务信息化管理中心</p>\n        </div>\n      </div>\n    </footer>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed, onMounted, watch } from 'vue'\nimport {\n  BarChart3,\n  Search,\n  Bell,\n  ChevronDown,\n  ChevronRight,\n  FileText,\n  PieChart,\n  Users,\n  Database,\n  BarChart,\n  Calculator,\n  FileCheck,\n  Building,\n  BookOpen,\n  CreditCard,\n  Landmark,\n  Receipt,\n  TrendingUp,\n  AlertCircle,\n  HelpCircle,\n  X,\n  Eye,\n  Brain,\n  UserCog,\n  Monitor,\n  Flag,\n  Globe,\n  Presentation,\n  MessageSquare,\n  Star,\n  LifeBuoy,\n  Briefcase,\n  Scale\n} from 'lucide-vue-next'\nimport axios from 'axios'\n\n// 用户登录状态\nconst currentUser = ref(null)\nconst showLoginModal = ref(false)\nconst loginForm = ref({\n  username: '',\n  password: ''\n})\nconst loginError = ref('')\nconst isLoggingIn = ref(false)\nconst showUserMenu = ref(false)\n\n// 查看全部功能模态框\nconst showAllAppsModal = ref(false)\n\n// 收藏夹\nconst favorites = ref([])\n\n// 检查是否已登录和加载收藏夹\nonMounted(() => {\n  // 检查登录状态\n  const savedUser = localStorage.getItem('currentUser')\n  if (savedUser) {\n    try {\n      currentUser.value = JSON.parse(savedUser)\n    } catch (e) {\n      localStorage.removeItem('currentUser')\n    }\n  }\n  \n  // 加载收藏夹\n  const savedFavorites = localStorage.getItem('favorites')\n  if (savedFavorites) {\n    try {\n      favorites.value = JSON.parse(savedFavorites)\n    } catch (e) {\n      localStorage.removeItem('favorites')\n    }\n  }\n})\n\n// 收藏功能\nconst toggleFavorite = (app) => {\n  const index = favorites.value.findIndex(fav => fav.name === app.name)\n  if (index === -1) {\n    // 添加到收藏\n    favorites.value.push(app)\n  } else {\n    // 从收藏中移除\n    favorites.value.splice(index, 1)\n  }\n  // 保存到本地存储\n  localStorage.setItem('favorites', JSON.stringify(favorites.value))\n}\n\n// 检查应用是否已收藏\nconst isFavorite = (app) => {\n  return favorites.value.some(fav => fav.name === app.name)\n}\n\n// 登录处理\nconst handleLogin = async () => {\n  try {\n    isLoggingIn.value = true\n    loginError.value = ''\n    \n    // 调用后端API进行登录验证\n    const response = await axios.post('/api/login', {\n      username: loginForm.value.username,\n      password: loginForm.value.password\n    })\n    \n    // 登录成功\n    currentUser.value = {\n      id: response.data.id,\n      username: response.data.username,\n      role: response.data.role\n    }\n    \n    // 保存用户信息到本地存储\n    localStorage.setItem('currentUser', JSON.stringify(currentUser.value))\n    \n    // 关闭登录模态框\n    showLoginModal.value = false\n    \n    // 重置表单\n    loginForm.value = {\n      username: '',\n      password: ''\n    }\n  } catch (error) {\n    console.error('登录失败:', error)\n    loginError.value = error.response?.data?.message || '登录失败，请检查用户名和密码'\n  } finally {\n    isLoggingIn.value = false\n  }\n}\n\n// 退出登录\nconst handleLogout = () => {\n  currentUser.value = null\n  localStorage.removeItem('currentUser')\n  showUserMenu.value = false\n}\n\n// 用户菜单状态\nconst toggleUserMenu = () => {\n  showUserMenu.value = !showUserMenu.value\n}\n\n// 搜索\nconst searchQuery = ref('')\n\n// 通知\nconst notifications = [\n  { content: '关于开展2025年度税收大数据分析工作的通知', time: '2025-05-05' },\n  { content: '智税平台V2.3版本更新说明', time: '2025-05-03' },\n  { content: '关于加强税收风险管理的工作指引', time: '2025-04-28' },\n  { content: '第三季度税收数据分析报告已发布', time: '2025-04-25' }\n]\n\n// 全部应用\nconst allApplications = [\n  { name: '指标全生命周期管理', icon: LifeBuoy, color: 'blue', description: '指标管理与监控平台', url: '#' },\n  { name: '智税展示', icon: Presentation, color: 'green', description: '税务数据可视化展示', url: '#' },\n  { name: '税费服务诉求', icon: MessageSquare, color: 'purple', description: '税费服务与诉求处理', url: '#' },\n  { name: '税费融合', icon: CreditCard, color: 'orange', description: '税费业务一体化管理', url: '#' },\n  { name: '慧眼', icon: Eye, color: 'red', description: '税务风险智能监控', url: '#' },\n  { name: '数智人事', icon: UserCog, color: 'indigo', description: '人事管理数字化平台', url: