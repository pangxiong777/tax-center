{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { BarChart3, Search, Bell, ChevronDown, ChevronRight, FileText, PieChart, Users, Database, BarChart, Calculator, FileCheck, Building, BookOpen, CreditCard, Landmark, Receipt, TrendingUp, AlertCircle, HelpCircle } from 'lucide-vue-next';\n\n// 用户菜单状态\n\nexport default {\n  __name: 'TaxCenter',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const showUserMenu = ref(false);\n    const toggleUserMenu = () => {\n      showUserMenu.value = !showUserMenu.value;\n    };\n\n    // 搜索\n    const searchQuery = ref('');\n\n    // 快速导航链接（现在是收藏夹）\n    const quickLinks = [{\n      name: '申报管理',\n      icon: FileText\n    }, {\n      name: '数据分析',\n      icon: PieChart\n    }, {\n      name: '纳税人管理',\n      icon: Users\n    }, {\n      name: '税收数据库',\n      icon: Database\n    }, {\n      name: '政策法规',\n      icon: BookOpen\n    }];\n\n    // 通知\n    const notifications = [{\n      content: '关于开展2025年度税收大数据分析工作的通知',\n      time: '2025-05-05'\n    }, {\n      content: '智税平台V2.3版本更新说明',\n      time: '2025-05-03'\n    }, {\n      content: '关于加强税收风险管理的工作指引',\n      time: '2025-04-28'\n    }, {\n      content: '第三季度税收数据分析报告已发布',\n      time: '2025-04-25'\n    }];\n\n    // 常用应用\n    const featuredApps = [{\n      name: '税收申报',\n      icon: FileCheck,\n      color: 'blue',\n      description: '电子税务局申报系统',\n      url: 'http://www.baidu.com'\n    }, {\n      name: '发票管理',\n      icon: Receipt,\n      color: 'green',\n      description: '电子发票开具与管理'\n    }, {\n      name: '税收分析',\n      icon: BarChart,\n      color: 'purple',\n      description: '税收数据可视化分析'\n    }, {\n      name: '纳税人画像',\n      icon: Users,\n      color: 'orange',\n      description: '纳税人信用与风险评估'\n    }, {\n      name: '政策查询',\n      icon: BookOpen,\n      color: 'red',\n      description: '税收政策法规库'\n    }, {\n      name: '税收计算器',\n      icon: Calculator,\n      color: 'indigo',\n      description: '各类税费快速计算'\n    }, {\n      name: '企业监管',\n      icon: Building,\n      color: 'yellow',\n      description: '企业税务监管平台'\n    }, {\n      name: '金融税务',\n      icon: CreditCard,\n      color: 'pink',\n      description: '金融业税收管理'\n    }];\n\n    // 应用分类\n    const categories = [{\n      id: 'all',\n      name: '全部'\n    }, {\n      id: 'collection',\n      name: '征收管理'\n    }, {\n      id: 'inspection',\n      name: '稽查管理'\n    }, {\n      id: 'service',\n      name: '纳税服务'\n    }, {\n      id: 'analysis',\n      name: '数据分析'\n    }, {\n      id: 'risk',\n      name: '风险管理'\n    }, {\n      id: 'policy',\n      name: '政策法规'\n    }];\n    const activeCategory = ref('all');\n\n    // 所有应用\n    const allApps = [\n    // 征收管理\n    {\n      name: '申报管理系统',\n      icon: FileText,\n      color: 'blue',\n      category: 'collection',\n      description: '纳税申报与管理'\n    }, {\n      name: '发票管理平台',\n      icon: Receipt,\n      color: 'green',\n      category: 'collection',\n      description: '发票开具与管理'\n    }, {\n      name: '税款征收系统',\n      icon: CreditCard,\n      color: 'indigo',\n      category: 'collection',\n      description: '税款征收与入库'\n    }, {\n      name: '减免税管理',\n      icon: Calculator,\n      color: 'red',\n      category: 'collection',\n      description: '税收减免申请与审批'\n    },\n    // 稽查管理\n    {\n      name: '稽查案件管理',\n      icon: FileCheck,\n      color: 'orange',\n      category: 'inspection',\n      description: '税务稽查案件管理'\n    }, {\n      name: '税收风险监控',\n      icon: AlertCircle,\n      color: 'red',\n      category: 'inspection',\n      description: '税收风险实时监控'\n    }, {\n      name: '稽查分析工具',\n      icon: PieChart,\n      color: 'purple',\n      category: 'inspection',\n      description: '稽查数据分析工具'\n    },\n    // 纳税服务\n    {\n      name: '纳税人服务平台',\n      icon: Users,\n      color: 'blue',\n      category: 'service',\n      description: '纳税人在线服务'\n    }, {\n      name: '税收政策咨询',\n      icon: HelpCircle,\n      color: 'green',\n      category: 'service',\n      description: '税收政策在线咨询'\n    }, {\n      name: '办税指南',\n      icon: BookOpen,\n      color: 'indigo',\n      category: 'service',\n      description: '办税流程与指南'\n    },\n    // 数据分析\n    {\n      name: '税收数据分析',\n      icon: BarChart,\n      color: 'purple',\n      category: 'analysis',\n      description: '税收数据多维分析'\n    }, {\n      name: '税收预测模型',\n      icon: TrendingUp,\n      color: 'green',\n      category: 'analysis',\n      description: '税收趋势预测'\n    }, {\n      name: '区域税收分析',\n      icon: PieChart,\n      color: 'blue',\n      category: 'analysis',\n      description: '区域税收对比分析'\n    }, {\n      name: '行业税负分析',\n      icon: BarChart3,\n      color: 'orange',\n      category: 'analysis',\n      description: '行业税负水平分析'\n    },\n    // 风险管理\n    {\n      name: '风险识别系统',\n      icon: AlertCircle,\n      color: 'red',\n      category: 'risk',\n      description: '税收风险智能识别'\n    }, {\n      name: '纳税人信用评级',\n      icon: Users,\n      color: 'blue',\n      category: 'risk',\n      description: '纳税人信用等级评定'\n    }, {\n      name: '风险应对策略',\n      icon: FileCheck,\n      color: 'green',\n      category: 'risk',\n      description: '税收风险应对策略'\n    },\n    // 政策法规\n    {\n      name: '税收法规库',\n      icon: BookOpen,\n      color: 'indigo',\n      category: 'policy',\n      description: '税收法律法规查询'\n    }, {\n      name: '政策解读平台',\n      icon: FileText,\n      color: 'purple',\n      category: 'policy',\n      description: '税收政策专业解读'\n    }, {\n      name: '税收案例库',\n      icon: Landmark,\n      color: 'blue',\n      category: 'policy',\n      description: '典型税收案例分析'\n    }];\n\n    // 根据分类筛选应用\n    const filteredApps = computed(() => {\n      if (activeCategory.value === 'all') {\n        return allApps;\n      } else {\n        return allApps.filter(app => app.category === activeCategory.value);\n      }\n    });\n    const __returned__ = {\n      showUserMenu,\n      toggleUserMenu,\n      searchQuery,\n      quickLinks,\n      notifications,\n      featuredApps,\n      categories,\n      activeCategory,\n      allApps,\n      filteredApps,\n      ref,\n      computed,\n      get BarChart3() {\n        return BarChart3;\n      },\n      get Search() {\n        return Search;\n      },\n      get Bell() {\n        return Bell;\n      },\n      get ChevronDown() {\n        return ChevronDown;\n      },\n      get ChevronRight() {\n        return ChevronRight;\n      },\n      get FileText() {\n        return FileText;\n      },\n      get PieChart() {\n        return PieChart;\n      },\n      get Users() {\n        return Users;\n      },\n      get Database() {\n        return Database;\n      },\n      get BarChart() {\n        return BarChart;\n      },\n      get Calculator() {\n        return Calculator;\n      },\n      get FileCheck() {\n        return FileCheck;\n      },\n      get Building() {\n        return Building;\n      },\n      get BookOpen() {\n        return BookOpen;\n      },\n      get CreditCard() {\n        return CreditCard;\n      },\n      get Landmark() {\n        return Landmark;\n      },\n      get Receipt() {\n        return Receipt;\n      },\n      get TrendingUp() {\n        return TrendingUp;\n      },\n      get AlertCircle() {\n        return AlertCircle;\n      },\n      get HelpCircle() {\n        return HelpCircle;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","BarChart3","Search","Bell","ChevronDown","ChevronRight","FileText","PieChart","Users","Database","BarChart","Calculator","FileCheck","Building","BookOpen","CreditCard","Landmark","Receipt","TrendingUp","AlertCircle","HelpCircle","showUserMenu","toggleUserMenu","value","searchQuery","quickLinks","name","icon","notifications","content","time","featuredApps","color","description","url","categories","id","activeCategory","allApps","category","filteredApps","filter","app"],"sources":["C:/Users/fatbear/Desktop/tax-center/src/components/TaxCenter.vue"],"sourcesContent":["<template>\n  <div class=\"tax-center-app bg-gray-50 min-h-screen\">\n    <!-- 顶部导航栏 -->\n    <header class=\"bg-white shadow-sm\">\n      <div class=\"container mx-auto px-4\">\n        <div class=\"flex items-center justify-between h-16\">\n          <div class=\"flex items-center\">\n            <div class=\"flex-shrink-0\">\n              <div class=\"flex items-center\">\n                <BarChart3 class=\"h-8 w-8 text-blue-600\" />\n                <span class=\"ml-2 text-xl font-bold text-gray-900\">强基中心</span>\n              </div>\n            </div>\n            <!-- 导航链接已移除 -->\n          </div>\n          <div class=\"flex items-center\">\n            <div class=\"relative\">\n              <input\n                v-model=\"searchQuery\"\n                type=\"text\"\n                placeholder=\"搜索应用...\"\n                class=\"bg-gray-100 rounded-full py-2 pl-10 pr-4 w-64 focus:outline-none focus:ring-2 focus:ring-blue-500 focus:bg-white\"\n              />\n              <div class=\"absolute left-3 top-2.5\">\n                <Search class=\"h-5 w-5 text-gray-400\" />\n              </div>\n            </div>\n            <div class=\"ml-4 flex items-center\">\n              <Bell class=\"h-6 w-6 text-gray-500 hover:text-blue-600 cursor-pointer\" />\n              <!-- 用户头像和下拉菜单 -->\n              <div class=\"ml-4 relative\">\n                <div class=\"flex items-center cursor-pointer\" @click=\"toggleUserMenu\">\n                  <img\n                    class=\"h-8 w-8 rounded-full\"\n                    src=\"/images/1.jpg\"\n                    alt=\"用户头像\"\n                  />\n                  <span class=\"ml-2 text-sm font-medium text-gray-700 hidden md:block\">王税务</span>\n                  <ChevronDown class=\"ml-1 h-4 w-4 text-gray-500\" />\n                </div>\n                <div\n                  v-if=\"showUserMenu\"\n                  class=\"origin-top-right absolute right-0 mt-2 w-48 rounded-md shadow-lg bg-white ring-1 ring-black ring-opacity-5 z-10\"\n                >\n                  <div class=\"py-1\">\n                    <a href=\"#\" class=\"block px-4 py-2 text-sm text-gray-700 hover:bg-gray-100\">退出登录</a>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </header>\n\n    <!-- 主要内容区 -->\n    <main class=\"container mx-auto px-4 py-6\">\n      <div class=\"flex flex-col md:flex-row\">\n        <!-- 左侧边栏 -->\n        <div class=\"w-full md:w-64 mb-6 md:mb-0 md:mr-6\">\n          <!-- 公告栏移到上面 -->\n          <div class=\"bg-white rounded-lg shadow p-4 mb-6\">\n            <h3 class=\"text-lg font-medium text-gray-900 mb-3\">公告栏</h3>\n            <div class=\"space-y-3\">\n              <div v-for=\"(notice, index) in notifications\" :key=\"index\" class=\"border-b border-gray-100 pb-3 last:border-0 last:pb-0\">\n                <div class=\"text-sm text-gray-600\">{{ notice.content }}</div>\n                <div class=\"text-xs text-gray-400 mt-1\">{{ notice.time }}</div>\n              </div>\n            </div>\n          </div>\n          \n          <!-- 快速导航改为收藏夹 -->\n          <div class=\"bg-white rounded-lg shadow p-4\">\n            <h3 class=\"text-lg font-medium text-gray-900 mb-3\">收藏夹</h3>\n            <ul class=\"space-y-2\">\n              <li v-for=\"(item, index) in quickLinks\" :key=\"index\">\n                <a href=\"#\" class=\"flex items-center text-gray-600 hover:text-blue-600 py-1.5\">\n                  <component :is=\"item.icon\" class=\"h-5 w-5 mr-2\" />\n                  <span>{{ item.name }}</span>\n                </a>\n              </li>\n            </ul>\n          </div>\n        </div>\n\n        <!-- 主内容区 -->\n        <div class=\"flex-1\">\n          <!-- 轮播图 -->\n          <div class=\"bg-white rounded-lg shadow mb-6 overflow-hidden\">\n            <div class=\"relative h-64\">\n              <div class=\"absolute inset-0 flex\">\n                <div class=\"w-full h-full bg-gradient-to-r from-blue-600 to-blue-800 flex items-center\">\n                  <div class=\"px-8 md:px-12 w-full md:w-1/2\">\n                    <h2 class=\"text-2xl md:text-3xl font-bold text-white mb-4\">智慧税务 数字赋能</h2>\n                    <p class=\"text-white text-opacity-90 mb-6\">\n                      整合税务系统资源，提供一站式税务服务平台，助力税收治理现代化\n                    </p>\n                    <button class=\"bg-white text-blue-600 px-4 py-2 rounded-md font-medium hover:bg-gray-100\">\n                      了解更多\n                    </button>\n                  </div>\n                </div>\n                <div class=\"hidden md:block w-1/2 h-full bg-center bg-cover\" style=\"background-image: url('/images/2.avif')\"></div>\n              </div>\n            </div>\n          </div>\n\n          <!-- 常用应用 -->\n          <div class=\"mb-6\">\n            <div class=\"flex items-center justify-between mb-4\">\n              <h2 class=\"text-xl font-bold text-gray-900\">常用应用</h2>\n              <a href=\"#\" class=\"text-sm text-blue-600 hover:text-blue-700 flex items-center\">\n                查看全部 <ChevronRight class=\"h-4 w-4 ml-1\" />\n              </a>\n            </div>\n            <div class=\"grid grid-cols-2 sm:grid-cols-3 md:grid-cols-4 gap-4\">\n              <a\n                v-for=\"(app, index) in featuredApps\"\n                :key=\"index\"\n                :href=\"app.url || '#'\"\n                :target=\"app.url ? '_blank' : ''\"\n                class=\"bg-white rounded-lg shadow p-4 hover:shadow-md transition-shadow cursor-pointer no-underline\"\n              >\n                <div class=\"flex flex-col items-center\">\n                  <div :class=\"`bg-${app.color}-100 p-3 rounded-lg mb-3`\">\n                    <component :is=\"app.icon\" class=\"h-6 w-6\" :class=\"`text-${app.color}-600`\" />\n                  </div>\n                  <div class=\"text-center\">\n                    <h3 class=\"font-medium text-gray-900\">{{ app.name }}</h3>\n                    <p class=\"text-xs text-gray-500 mt-1\">{{ app.description }}</p>\n                  </div>\n                </div>\n              </a>\n            </div>\n          </div>\n\n          <!-- 应用分类 -->\n          <div>\n            <div class=\"mb-4\">\n              <h2 class=\"text-xl font-bold text-gray-900\">应用分类</h2>\n            </div>\n            \n            <div class=\"bg-white rounded-lg shadow overflow-hidden\">\n              <div class=\"border-b border-gray-200\">\n                <div class=\"flex\">\n                  <button\n                    v-for=\"(category, index) in categories\"\n                    :key=\"index\"\n                    @click=\"activeCategory = category.id\"\n                    :class=\"[\n                      'px-4 py-3 text-sm font-medium whitespace-nowrap',\n                      activeCategory === category.id\n                        ? 'text-blue-600 border-b-2 border-blue-600'\n                        : 'text-gray-500 hover:text-gray-700'\n                    ]\"\n                  >\n                    {{ category.name }}\n                  </button>\n                </div>\n              </div>\n              \n              <div class=\"p-4\">\n                <div class=\"grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 gap-4\">\n                  <div\n                    v-for=\"(app, index) in filteredApps\"\n                    :key=\"index\"\n                    class=\"border border-gray-200 rounded-lg p-4 hover:border-blue-300 hover:bg-blue-50 transition-colors cursor-pointer\"\n                  >\n                    <div class=\"flex items-start\">\n                      <div :class=\"`bg-${app.color}-100 p-2 rounded-lg`\">\n                        <component :is=\"app.icon\" class=\"h-5 w-5\" :class=\"`text-${app.color}-600`\" />\n                      </div>\n                      <div class=\"ml-3\">\n                        <h3 class=\"font-medium text-gray-900\">{{ app.name }}</h3>\n                        <p class=\"text-xs text-gray-500 mt-1\">{{ app.description }}</p>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </main>\n\n    <!-- 底部 -->\n    <footer class=\"bg-white border-t border-gray-200 mt-8\">\n      <div class=\"container mx-auto px-4 py-6\">\n        <div class=\"text-center text-gray-500 text-sm\">\n          <p>© 2025 国家税务总局 智税中心. 版权所有</p>\n          <p class=\"mt-1\">技术支持: 税务信息化管理中心</p>\n        </div>\n      </div>\n    </footer>\n  </div>\n</template>\n\n<script setup>\nimport { ref, computed } from 'vue'\nimport {\n  BarChart3,\n  Search,\n  Bell,\n  ChevronDown,\n  ChevronRight,\n  FileText,\n  PieChart,\n  Users,\n  Database,\n  BarChart,\n  Calculator,\n  FileCheck,\n  Building,\n  BookOpen,\n  CreditCard,\n  Landmark,\n  Receipt,\n  TrendingUp,\n  AlertCircle,\n  HelpCircle\n} from 'lucide-vue-next'\n\n// 用户菜单状态\nconst showUserMenu = ref(false)\nconst toggleUserMenu = () => {\n  showUserMenu.value = !showUserMenu.value\n}\n\n// 搜索\nconst searchQuery = ref('')\n\n// 快速导航链接（现在是收藏夹）\nconst quickLinks = [\n  { name: '申报管理', icon: FileText },\n  { name: '数据分析', icon: PieChart },\n  { name: '纳税人管理', icon: Users },\n  { name: '税收数据库', icon: Database },\n  { name: '政策法规', icon: BookOpen }\n]\n\n// 通知\nconst notifications = [\n  { content: '关于开展2025年度税收大数据分析工作的通知', time: '2025-05-05' },\n  { content: '智税平台V2.3版本更新说明', time: '2025-05-03' },\n  { content: '关于加强税收风险管理的工作指引', time: '2025-04-28' },\n  { content: '第三季度税收数据分析报告已发布', time: '2025-04-25' }\n]\n\n// 常用应用\nconst featuredApps = [\n  { name: '税收申报', icon: FileCheck, color: 'blue', description: '电子税务局申报系统', url: 'http://www.baidu.com' },\n  { name: '发票管理', icon: Receipt, color: 'green', description: '电子发票开具与管理' },\n  { name: '税收分析', icon: BarChart, color: 'purple', description: '税收数据可视化分析' },\n  { name: '纳税人画像', icon: Users, color: 'orange', description: '纳税人信用与风险评估' },\n  { name: '政策查询', icon: BookOpen, color: 'red', description: '税收政策法规库' },\n  { name: '税收计算器', icon: Calculator, color: 'indigo', description: '各类税费快速计算' },\n  { name: '企业监管', icon: Building, color: 'yellow', description: '企业税务监管平台' },\n  { name: '金融税务', icon: CreditCard, color: 'pink', description: '金融业税收管理' }\n]\n\n// 应用分类\nconst categories = [\n  { id: 'all', name: '全部' },\n  { id: 'collection', name: '征收管理' },\n  { id: 'inspection', name: '稽查管理' },\n  { id: 'service', name: '纳税服务' },\n  { id: 'analysis', name: '数据分析' },\n  { id: 'risk', name: '风险管理' },\n  { id: 'policy', name: '政策法规' }\n]\n\nconst activeCategory = ref('all')\n\n// 所有应用\nconst allApps = [\n  // 征收管理\n  { name: '申报管理系统', icon: FileText, color: 'blue', category: 'collection', description: '纳税申报与管理' },\n  { name: '发票管理平台', icon: Receipt, color: 'green', category: 'collection', description: '发票开具与管理' },\n  { name: '税款征收系统', icon: CreditCard, color: 'indigo', category: 'collection', description: '税款征收与入库' },\n  { name: '减免税管理', icon: Calculator, color: 'red', category: 'collection', description: '税收减免申请与审批' },\n  \n  // 稽查管理\n  { name: '稽查案件管理', icon: FileCheck, color: 'orange', category: 'inspection', description: '税务稽查案件管理' },\n  { name: '税收风险监控', icon: AlertCircle, color: 'red', category: 'inspection', description: '税收风险实时监控' },\n  { name: '稽查分析工具', icon: PieChart, color: 'purple', category: 'inspection', description: '稽查数据分析工具' },\n  \n  // 纳税服务\n  { name: '纳税人服务平台', icon: Users, color: 'blue', category: 'service', description: '纳税人在线服务' },\n  { name: '税收政策咨询', icon: HelpCircle, color: 'green', category: 'service', description: '税收政策在线咨询' },\n  { name: '办税指南', icon: BookOpen, color: 'indigo', category: 'service', description: '办税流程与指南' },\n  \n  // 数据分析\n  { name: '税收数据分析', icon: BarChart, color: 'purple', category: 'analysis', descript